  text-align: center;
  text-shadow: 0 1px 0 rgba(255, 255, 255, 0.6);
}
#header {
  box-sizing: border-box;
  overflow: hidden;
  padding: 15px 0 10px;
  position: relative;
}
.logoLink span {
