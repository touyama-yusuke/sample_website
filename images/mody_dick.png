.wrapperForm p {
  font-family: "proxima-nova",sans-serif;
  font-size: 14px;
  line-height: 1.6;
}
.le-error {
  display: flex;
  align-items: center;
  justify-content: center;
}
.le-error img {
    height: 600px;
}
.fancylink {
  box-shadow: 0 -2px 0 0 rgb(114 146 176 / 30%) inset, 0 1px 0 0 rgb(114 146 176 / 30%);
